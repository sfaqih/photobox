{
  "name": "photobox",
  "private": true,
  "version": "1.0.0",
  "author": "Sultan Faqih",
  "description": " PhotoBox Pixify Application",
  "type": "module",
  "scripts": {
    "electron": "electron electron/main.cjs",
    "vite-build": "vite build",
    "dev": "vite",
    "lint": "eslint .",
    "preview": "vite preview",
    "dist-signed": "npm run vite-build  --enable-touch-events && electron-builder build --win --x64 --publish never"
  },
  "dependencies": {
    "@material-tailwind/react": "^2.1.10",
    "@react-spring/konva": "^10.0.1",
    "@tailwindcss/vite": "^4.1.4",
    "@use-gesture/react": "^10.3.1",
    "@vitejs/plugin-react-swc": "3.8.0",
    "electron-log": "^5.4.0",
    "image-size": "^2.0.2",
    "konva": "^9.3.20",
    "lucide-react": "^0.507.0",
    "minimatch": "8.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-idle-timer": "^5.7.2",
    "react-konva": "18.2.10",
    "react-rnd": "^10.5.2",
    "react-router-dom": "6.27.0",
    "sharp": "^0.34.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@types/react": "^18.2.10",
    "@types/react-dom": "18",
    "autoprefixer": "^10.4.21",
    "electron": "^35.2.0",
    "electron-builder": "^26.0.12",
    "eslint": "^9.22.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "postcss": "^8.5.3",
    "tailwind-scrollbar": "^4.0.2",
    "tailwindcss": "^4.1.4",
    "vite": "^6.3.1"
  },
  "resolutions": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "minimatch": "^8.0.2"
  },
  "main": "electron/main.cjs",
  "build": {
    "asar": true,
    "appId": "id.pixify.photobox",
    "productName": "PhotoBox",
    "copyright": "Copyright Â© 2025 Pixify",
    "asarUnpack": [
      "**/node_modules/sharp/**/*",
      "**/node_modules/@img/**/*",
      "node_modules/sharp/**/*",
      "node_modules/@img/**/*",
      "electron/data/*"
    ],
    "files": [
      "react-dist/",
      "electron/",
      "public/",
      "!src/"
    ],
    "mac": {
      "target": "dmg",
      "category": "public.app-category.photography"
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "public/icons/icon-256x256.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "perMachine": true,
      "installerIcon": "public/icons/icon-256x256.ico",
      "uninstallerIcon": "public/icons/icon-256x256.ico",
      "license": "LICENSE.txt",
      "runAfterFinish": true
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "packageManager": "yarn@4.9.1"
}
